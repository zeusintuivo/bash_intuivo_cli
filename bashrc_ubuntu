_git_log_prettily () {
    if ! [ -z $1 ]; then
        git log --pretty=$1
    fi
 }
 colorize_via_pygmentize () {
    if [ ! -x "$(which pygmentize)" ]; then
        return -1
    fi
    if [ $# -eq 0 ]; then
        pygmentize -g $@
    fi
    for FNAME in $@; do
        filename=$(basename "$FNAME")
        lexer=`pygmentize -N \"$filename\"`
        if [ "Z$lexer" != "Ztext" ]; then
            pygmentize -l $lexer "$FNAME"
        else
            pygmentize -g "$FNAME"
        fi
    done
 }
