 case  $(whoami) in
        (root*)    _user="\[\033[01;$R_E_D\]\u" ;;
        (je*)      _user="\[\033[01;$D_A_R_K_B_L_U_E\]\u" ;;
        (vagrant*) _user="\[\033[01;$Y_E_LL_O_W\]\u" ;;
        (docker*)  _user="\[\033[01;$G_R_A_Y_E_D_O_U_T\]\u" ;;
        (zeus*)    _user="\[\033[01;$P_U_R_P_L_E\]\u" ;;
        (*)        _user="\[\033[01;$G_R_E_E_N\]\u" ;;
 esac

 
